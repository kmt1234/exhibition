<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="businessRoomSQL">
 	<select id="getCalendar" parameterType="String" resultType="rental.bean.BusinessRoomDTO">
 		select * from businessRoom where M_Id = #{businessRoom}
 	</select>
 	
 	<select id="getTimeListBusinessRoom" parameterType="java.util.Map" resultType="rental.bean.BusinessRoomDTO">
 		select first, second, third, fourth from BUSINESSROOM where roomName=#{roomName} and STARTDATE=#{startDate}
 	</select>
</mapper>